<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
				Rastreo GPS
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link rel="Shortcut Icon" type="image/x-icon" href="{{ asset('img/principal.png') }}"/>
			<link rel="stylesheet" href="{{ asset('css/vista/sweet-alert.css') }}">
			<link rel="stylesheet" href="{{ asset('css/vista/material-design-iconic-font.min.css') }}">
			<link rel="stylesheet" href="{{ asset('css/vista/normalize.css') }}">
			<link rel="stylesheet" href="{{ asset('css/vista/bootstrap.min.css') }}">
			<link rel="stylesheet" href="{{ asset('css/vista/jquery.mCustomScrollbar.css') }}">
			<link rel="stylesheet" href="{{ asset('css/vista/style.css') }}">
			<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
			<script>
				window.jQuery || document.write('<script src="      {{ asset('js/vista/jquery-1.11.2.min.js') }}"><\/script>')
			</script>
			<script src="{{ asset('js/vista/modernizr.js') }}"></script>
			<script src="{{ asset('js/vista/bootstrap.min.js') }}"></script>
			<script src="{{ asset('js/vista/jquery.mCustomScrollbar.concat.min.js') }}"></script>
			<script src="{{ asset('js/vista/main.js') }}"></script>

		{% endblock %}
	</head>

	<body>
		<div class="navbar-lateral full-reset">
			<div class="full-reset container-menu-movile custom-scroll-containers">
				<div class="full-reset container-menu-movile custom-scroll-containers">
					{% block navbar %}
						<!-- Contenido de la barra de navegación lateral -->
						<!-- Logo -->
							<div class="logo full-reset all-tittles"> <i class="visible-xs zmdi zmdi-close pull-left mobile-menu-button" style="line-height: 55px; cursor: pointer; padding: 0 10px; margin-left: 7px;"></i>
						</div>

						<!-- Encabezado con imagen de fondo -->
						<div class="full-reset" style="background-color:#2B3D51; padding: 10px 0; color:#fff;">
							<figure>
								<img src="{{ asset('img/app/Logo1.jpeg') }}" alt="TrackLogo" class="img-responsive center-box" style="width:55%;">
							</figure>
							<p class="text-center" style="padding-top: 20px;">
								Track Master
							</p>
						</div>
					{% endblock %}
				</div>
			</div>
		</div>
		<div class="content-page-container full-reset custom-scroll-containers">
			<nav class="navbar-user-top full-reset">
				{% block topbar %}
					<!-- Contenido de la barra de navegación superior -->
					
				{% endblock %}
			</nav>
	
		<div class="container">
			<!-- Contenedor del mapa -->
			{% block container %}
			
        </div>
			{% endblock %}
		</div>
	</div>
		<div class="modal fade" tabindex="-1" role="dialog" id="ModalHelp">
			<div class="modal-dialog modal-lg">
				<div class="modal-content">
					{% block modal_content %}
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<span aria-hidden="true">
									&times;
								</span>
							</button>
							<h4 class="modal-title text-center all-tittles">
								Ayuda del sistema
							</h4>
						</div>
						<div class="modal-body"></div>
						<div class="modal-footer">
							<button type="button" class="btn btn-primary" data-dismiss="modal">
								<i class="zmdi zmdi-thumb-up"></i>
								&nbsp; De acuerdo
							</button>
						</div>
					{% endblock %}
				</div>
			</div>
		</div>
		<footer class="footer full-reset">
			<div class="container-fluid">
				{% block footer %}
					<!-- Contenido del pie de página -->
					<div class="row">
						<div class="col-xs-12 col-sm-6">
							<h4 class="all-tittles">
								Acerca de
							</h4>
						</div>
						<div class="col-xs-12 col-sm-6">
							<h4 class="all-tittles">
								Desarrollador
							</h4>
							<ul class="list-unstyled">
								<li>
									<i class="zmdi zmdi-check zmdi-hc-fw"></i>
									&nbsp;
									<i class="zmdi zmdi-facebook zmdi-hc-fw footer-social"></i>
									<i class="zmdi zmdi-twitter zmdi-hc-fw footer-social"></i>
								</li>
							</ul>
						</div>
					</div>
				{% endblock %}
			</div>
		</footer>
		</div>
	</body>
</html>
